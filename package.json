{
  "name": "zashiki-routing-middleware",
  "version": "1.0.17",
  "description": "Redux Routing middleware for Zashiki",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run babel && git add lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/modernpoacher/zashiki-routing-middleware.git"
  },
  "license": "MIT",
  "author": "Jonathan Perry for Modern Poacher Limited",
  "dependencies": {
    "shinkansen-pantograph": "1.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}

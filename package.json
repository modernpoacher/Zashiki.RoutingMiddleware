{
  "name": "zashiki-routing-middleware",
  "version": "1.0.14",
  "description": "Redux Routing middleware for Zashiki",
  "main": "./index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run babel && git add lib",
    "deps": "deps",
    "deps:prod": "deps --save-prod",
    "deps:dev": "deps --save-dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/modernpoacher/zashiki-routing-middleware.git"
  },
  "license": "MIT",
  "author": "Jonathan Perry for Modern Poacher Limited",
  "dependencies": {
    "shinkansen-pantograph": "1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@modernpoacher/deps": "0.0.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
